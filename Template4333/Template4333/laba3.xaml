<Window x:Class="Template4333.laba3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Template4333"
        mc:Ignorable="d"
        Title="laba3" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button Content="Импорт данных" x:Name="BtnImp" Click="BtnImp_Click" Grid.Column="0" Grid.Row="1"></Button>
        <Button Content="Экспорт в эксель" x:Name="BtnExp" Click="BtnExp_Click" Grid.Column="2" Grid.Row="1"></Button>
        <TextBlock x:Name="sss"></TextBlock>
    </Grid>
</Window>
